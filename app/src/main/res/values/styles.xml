<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="FirstPage" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@drawable/splashbeforload</item>
    </style>



    <style name="mlCheck">
        <item name="colorAccent">@color/mlHeader</item>
    </style>

    <style name="mySpinnerItemStyle"
        parent="@android:style/Widget.Holo.DropDownItem.Spinner">
        <item name="android:background">@color/mlFooter</item>
        <item name="android:textColor">@color/mlHeader</item>
    </style>

</resources>
