<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="verticaltextview">
        <attr name="direction">
            <enum name="uptodown" value="0" />
            <enum name="downtoup" value="1" />
            <enum name="lefttoright" value="2" />
            <enum name="righttoleft" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MLCurveHeader">
        <attr name="shadow" format="integer"/>
        <attr name="backcolor" format="color"/>
        <attr name="shadowcolor" format="color"/>
        <attr name="PitOpenings" format="integer"/>
    </declare-styleable>

</resources>